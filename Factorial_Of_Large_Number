import java.util.* ;
import java.io.*;
import java.math.BigInteger; 

public class Solution {
	public static String calculateFactorial(int n) {
                		 BigInteger f
            = new BigInteger("1"); 
 
        for (int i = 2; i <= n; i++)
            f = f.multiply(BigInteger.valueOf(i));
 
        return f.toString();
	}
}
